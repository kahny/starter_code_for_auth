<!DOCTYPE html>
<html>
<head>
	<title></title>
</head>
<body>
	<ul >
		<h1>Posts by user # <%= userId %> </h1>
        <% posts.forEach(function(post) { %>
        	<h3> <%= post.title %> </h3>
        	<p> <%= post.createdAt %> </p>
        	<p> <%= post.body %> </p>
        <% }) %>
    </ul>

    <!-- create new post from this user -->
    <br><br>
    <p><a href="/users/<%= userId %>/posts/new">Create a new post</a><p>
   	<p><a href="/users">User List</a></p>
   	<p><a href="/">Home</a><p>

</body>
</html>


	
